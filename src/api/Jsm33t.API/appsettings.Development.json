{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Async" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "Logs/log.txt",
                "rollingInterval": "Day"
              }
            }
          ]
        }
      }
    ]
  },
  "Jsm33tConfig": {
    "ConnectionString": "Data Source=localhost;Initial Catalog=jsm33t_db;Trusted_Connection=True;Integrated Security=SSPI;TrustServerCertificate=True",
    "JwtSettings": {
      "ValidIssuer": "https://api.jsm33t.me",
      "ValidAudience": "https://api.jsm33t.me",
      "IssuerSigningKey_": "yourissuerkey=====sds=="
    },
    "SmtpSettings": {
      "Server": "mail.maildomain.me",
      "Port": 1212,
      "Username": "username",
      "Password": "some pass",
      "FromName": "your mail",
      "FromEmail": "mail@mail.mail",
      "EnableSSL": true
    },
    "TelegramBot": {
      "BotToken": "nana",
      "LogChatId": "12121212",
      "LogToTele": "false"
    },
    "Paths": {
      "CDNURL": "http://localhost:8080"
    }
  },
  "RateLimiting": {
    "Global": {
      "PermitLimit": 100,
      "Window": "00:01:00",
      "QueueLimit": 0
    },
    "Routes": {
      "api/messages/send": {
        "PermitLimit": 20,
        "Window": "00:01:00",
        "QueueLimit": 0
      }
    }
  }
}
